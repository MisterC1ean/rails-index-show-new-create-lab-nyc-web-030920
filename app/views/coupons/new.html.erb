<%# <h1>This is the New Page</h1>
<h2>Coupon Form</h2>

<form action="/coupons" method="CREATE">
    <label for="coupon_code">Coupon Code:</label>
        <input type="text" id="coupon_code" name="coupon[coupon_code]">
    <label for="store">Store:</label>
        <input type="text" id="store" name="coupon[store]">
    <input type="submit" value="Submit Coupon">


</form> %>

<h3>Coupon Form</h3>

<%= form_tag coupons_path do %>
  <label>Coupon Code:</label><br>
  <%= text_field_tag :"coupon[coupon_code]" %><br>

  <label>Coupon for Store:</label><br>
  <%= text_field_tag :"coupon[store]" %><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

  <%= submit_tag "Submit Coupon" %>
<% end %>